          "rawSql": "SELECT \n  te.\"SourceAddress\" as \"IP Address\",\n  COUNT(*) as \"Total Attacks\",\n  COUNT(DISTINCT te.\"DestinationPort\") as \"Ports Targeted\",\n  COUNT(DISTINCT te.\"Category\") as \"Attack Types\",\n  COUNT(DISTINCT p.\"Name\") as \"Protocols Used\",\n  (COUNT(*) * COUNT(DISTINCT te.\"DestinationPort\") * COUNT(DISTINCT te.\"Category\")) as \"Attack Score\",\n  MIN(te.\"Timestamp\") as \"First Seen\",\n  MAX(te.\"Timestamp\") as \"Last Seen\"\nFROM \"ThreatEvents\" te\nJOIN \"Protocols\" p ON te.\"ProtocolId\" = p.\"Id\"\nWHERE te.\"DeletedAt\" IS NULL \n  AND $__timeFilter(te.\"Timestamp\")\nGROUP BY te.\"SourceAddress\"\nORDER BY \"Attack Score\" DESC\nLIMIT 20",
          "refId": "A"
        }
      ],
